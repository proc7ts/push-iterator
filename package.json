{
  "name": "@proc7ts/push-iterator",
  "version": "2.4.1",
  "description": "Push iteration protocol",
  "keywords": [
    "iteration",
    "iterator",
    "push-iteration",
    "push-iterator"
  ],
  "homepage": "https://github.com/proc7ts/push-iterator",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/push-iterator.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/push-iterator/issues"
  },
  "type": "module",
  "main": "dist/push-iterator.cjs",
  "module": "dist/push-iterator.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/push-iterator.cjs",
      "import": "./dist/push-iterator.js"
    },
    "./call-thru": {
      "require": "./dist/push-iterator.call-thru.cjs",
      "import": "./dist/push-iterator.call-thru.js"
    }
  },
  "peerDependencies": {
    "@proc7ts/call-thru": "^4.3.1"
  },
  "devDependencies": {
    "@proc7ts/call-thru": "^4.3.1",
    "@proc7ts/eslint-config": "^2.0.5",
    "@proc7ts/rollup-helpers": "^1.1.1",
    "@proc7ts/rollup-plugin-flat-dts": "^1.1.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.7",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.0",
    "codecov": "^3.8.1",
    "eslint": "^7.14.0",
    "eslint-plugin-jest": "^24.1.3",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.33.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "run-z": "=1.6.1-bootstrap",
    "shx": "^0.3.3",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "benchmark": "run-z benchmark:build --then node --no-warnings --enable-source-maps ./target/benchmark.js",
    "benchmark:build": "run-z --then rollup --config ./rollup.benchmark.config.js",
    "benchmark:record": "run-z benchmark:build benchmark:record:node12 benchmark:record:node14",
    "benchmark:record:node12": "LANG=C FORCE_COLOR=0 npx -q node@12 ./target/benchmark.mjs > ./benchmarks/node12.txt",
    "benchmark:record:node14": "LANG=C FORCE_COLOR=0 npx -q node@14 ./target/benchmark.mjs > ./benchmarks/node14.txt",
    "bootstrap": "rollup --config ./rollup.config.js",
    "build": "run-z +z bootstrap",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf index.d.ts '*/index.d.ts' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z --then jest",
    "z": "run-z +bootstrap,+doc,+lint,+test"
  }
}
